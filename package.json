{
  "name": "talljack-eslint-config",
  "type": "module",
  "version": "0.2.4",
  "packageManager": "bun@1.1.5",
  "description": "Talljack's eslint config",
  "author": "talljack <yugang.cao12@gmail.com> (https://github.com/talljack)",
  "license": "MIT",
  "funding": "https://github.com/sponsors/talljack",
  "homepage": "https://github.com/Talljack/talljack-eslint-config",
  "bugs": "https://github.com/Talljack/talljack-eslint-config/issues",
  "keywords": [
    "eslint",
    "eslint-config"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "build": "bun run typegen && tsup",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "dev": "npx @eslint/config-inspector --config eslint.config.ts",
    "build:inspector": "bun build && npx @eslint/config-inspector build",
    "watch": "tsup --format --esm,cjs --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "typegen": "esno scripts/genType.ts",
    "prepare": "simple-git-hooks"
  },
  "peerDependencies": {
    "eslint": ">= 9"
  },
  "dependencies": {
    "@antfu/install-pkg": "^0.3.3",
    "@clack/prompts": "^0.7.0",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "eslint-config-flat-gitignore": "^0.1.5",
    "eslint-flat-config-utils": "^0.2.4",
    "eslint-merge-processors": "^0.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-format": "^0.1.1",
    "eslint-plugin-import-x": "^0.5.0",
    "eslint-plugin-jsdoc": "^48.2.3",
    "eslint-plugin-jsonc": "^2.15.1",
    "eslint-plugin-markdown": "^4.0.1",
    "eslint-plugin-n": "^17.3.1",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.6",
    "eslint-plugin-sort": "^3.0.2",
    "eslint-plugin-unused-imports": "^3.1.0",
    "eslint-plugin-vitest": "^0.5.4",
    "eslint-plugin-vue": "^9.25.0",
    "eslint-plugin-yml": "^1.14.0",
    "eslint-processor-vue-blocks": "^0.1.2",
    "eslint-typegen": "^0.2.4",
    "local-pkg": "^0.5.0",
    "picocolors": "^1.0.0",
    "vue-eslint-parser": "^9.4.2",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.1.1",
    "@stylistic/eslint-plugin": "^1.7.2",
    "@stylistic/eslint-plugin-migrate": "^2.1.0",
    "@types/bun": "latest",
    "@types/eslint": "^8.56.10",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.12.7",
    "bundle-require": "^4.0.3",
    "eslint": "^9.1.1",
    "eslint-plugin-astro": "^1.0.3",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-solid": "^0.14.0",
    "eslint-plugin-svelte": "^2.38.0",
    "esno": "^4.7.0",
    "execa": "^8.0.1",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "globals": "^15.0.0",
    "lint-staged": "^15.2.2",
    "rimraf": "^5.0.5",
    "simple-git-hooks": "^2.11.1",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.7.1",
    "vue": "^3.4.25",
    "yaml-eslint-parser": "^1.2.2"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
